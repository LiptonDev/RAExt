  </ItemGroup>
  <ItemGroup>
    <Folder Include="Properties\PublishProfiles\" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="..\..\..\MetroParser\EmailExtractor\RealmExtensions.cs">
      <Link>RealmExtensions.cs</Link>
    </Compile>
  </ItemGroup>
